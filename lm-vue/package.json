  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run pub:entry && npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "node build/build.js",
    "pub:entry": "node build/bin/entry.js",
    "pub:conf": "webpack --config build/bin/conf.js",
    "pub:common": "webpack --config build/bin/common.js",
    "pub:component": "webpack --config build/bin/component.js",
    "pub:babel": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "pub:theme": "node build/bin/theme.js && gulp build --gulpfile src/theme/gulpfile.js && cp-cli src/theme/lib lib/theme",
    "public": "npm run pub:entry && npm run pub:conf && npm run pub:common && npm run pub:component && npm run pub:babel && npm run pub:theme"
